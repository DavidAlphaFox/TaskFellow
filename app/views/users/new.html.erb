<h2>Sign Up</h2>

<form class="new-user-form" action="<%= users_url %>" method="post">
  <%= auth_token %>

  <p>
    Username:
  </p>
  <input type="text" name="user[username]" value="">
  <p>
    Password:
  </p>
  <input type="password" name="user[password]" value="">
  <p>
    <button type="submit" name="button">Sign Up</button>
  </p>
</form>
